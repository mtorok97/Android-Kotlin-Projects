<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hu.bme.aut.android.hfdemo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_results2"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_results2"
        android:supportsRtl="true"
        android:theme="@style/Theme.Gameon"
        android:name=".PlayerApplication">
        <activity android:name=".TipViewActivity"/>
        <activity android:name=".DetailTipActivity" />
        <activity android:name=".TipActivity" />
        <activity android:name=".StartActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>